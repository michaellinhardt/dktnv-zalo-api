{
  "name": "dktvn-zalo-api",
  "version": "1.0.0",
  "description": "DKT VN Zalo API",
  "main": "dist/application/index.js",
  "author": "Michael Linhardt",
  "license": "ISC",
  "scripts": {
    "prestart": "npm run build",
    "start": "NODE_ENV=production node ./dist/application/index.js",
    "dev": "NODE_ENV=development npx nodemon --ignore ./application/knex --ignore ./src/test ./application/index.js --exec babel-node",
    "script": "NODE_ENV=development babel-node",
    "build": "rm -rf dist && npx babel src --out-dir dist/src --copy-files && npx babel application --out-dir dist/application --copy-files && npx babel knexfile.js --out-file dist/knexfile.js",
    "prod": "node dist/application/index.js",
    "lint": "npx eslint ./src ./application ./knexfile.js",
    "knex": "babel-node node_modules/.bin/knex",
    "mocha": "NODE_ENV=test npx mocha --recursive"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.22.1",
    "@babel/node": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@babel/register": "^7.21.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "chai-uuid": "^1.0.6",
    "core-js": "^3.30.2",
    "deep-equal-in-any-order": "^2.0.6",
    "env-test": "^1.0.0",
    "escope": "^4.0.0",
    "eslint": "^8.41.0",
    "eslint-config-zavatta": "^6.0.3",
    "eslint-plugin-mocha": "^10.1.0",
    "mocha": "^10.2.0",
    "node-mocks-http": "^1.12.2",
    "nodemon": "^2.0.22",
    "prettyjson": "^1.2.5",
    "sinon": "^15.1.0",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-errors": "^3.0.0",
    "bcrypt": "^5.1.0",
    "bluebird": "^3.7.2",
    "express": "^4.18.2",
    "jose": "^4.14.4",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mysql": "^2.18.1",
    "regenerator-runtime": "^0.13.11",
    "request-ip": "^3.3.0",
    "uuid": "^9.0.0"
  }
}
